            <string notr="true">QPushButton {
    background-color: #555555;
    color: #f0f0f0;
    border: none;
    border-radius: 8px;
    padding: 6px 14px;
    font-weight: 600;
    font-size: 12px;
    transition: background-color 0.15s ease;
}

/* Hover effect for unchecked buttons */
QPushButton:hover:!checked {
    background-color: #666666;
    color: #ffffff;
}

/* Hover effect for checked buttons */
QPushButton:hover:checked {
    background-color: #009999;
    border-color: #00cccc;
    color: #ffffff;
}

/* Pressed effect */
QPushButton:pressed {
    background-color: #222222;
    color: #ffffff;
    padding-top: 7px;
    padding-bottom: 5px;
    box-shadow: inset 0px 2px 4px rgba(0, 0, 0, 0.6);
}

/* Checked (toggled on) */
QPushButton:checked {
    background-color: #008080;
    color: #ffffff;
    font-weight: bold;
    border: 2px solid #00aaaa;
}

/* Unchecked (not toggled) */
QPushButton:!checked {
    background-color: #555555;
    color: #f0f0f0;
}

/* Optional: Selected state */
QPushButton:selected {
    background-color: white;
}
</string>
